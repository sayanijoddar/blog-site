<%- include('partials/header'); -%>

<h1>Home</h1>
<p> <%= startingContent %> </p>

<% homePosts.forEach(function(post){ %>
    <h1>
        <%= post.postTitle %>
    </h1>
    <p>
        <%= post.postBody.substring(0, 100) + "..." %>
        <a href="/posts/<%=post.postTitle%>">Read More</a>
    </p>
<% }) %>

<%- include('partials/footer'); -%>